<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceBlock name="cache.additional" >
			<block class="Icube\CustomStyle\Block\Cache\Additionalvarnish" name="cache.additional.varnish" template="Icube_CustomStyle::cache/additionalvarnish.phtml">
				<arguments>
                    <argument name="custompermissions" xsi:type="object">Icube\CustomStyle\Block\Cache\Permissions</argument>
                </arguments>
			</block>
			<block class="Icube\CustomStyle\Block\Cache\Additionaldeploy" name="cache.additional.deploy" template="Icube_CustomStyle::cache/additionaldeploy.phtml">
				<arguments>
                    <argument name="custompermissions" xsi:type="object">Icube\CustomStyle\Block\Cache\Permissions</argument>
                </arguments>
			</block>
			<block class="Icube\CustomStyle\Block\Cache\Additionalfpc" name="cache.additional.fpc" template="Icube_CustomStyle::cache/additionalfpc.phtml">
				<arguments>
                    <argument name="custompermissions" xsi:type="object">Icube\CustomStyle\Block\Cache\Permissions</argument>
                </arguments>
			</block>
		</referenceBlock>
	</body>
</page>